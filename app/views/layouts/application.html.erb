<!DOCTYPE html>
<html>
<head>
  <title>Practice Rails Application</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class="wrapper">
	<header class="navbar navbar-fixed-top navbar-inverse">
	  <div class="container">
	    <%= link_to "Practice App", root_path, id: "logo" %>
	    <nav>
	      <ul class="nav navbar-nav pull-right">
			<li><%= link_to "Cars", cars_path %></li>
			<li><%=	link_to "Owners", owners_path %></li>
	      </ul>
	    </nav>
	  </div>
	</header>
	<%= yield %>
	<div class="container">
		<%= debug @project %>
	</div>
</div>
</body>
</html>
